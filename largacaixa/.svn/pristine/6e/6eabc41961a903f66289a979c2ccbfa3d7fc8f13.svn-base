package pt.largacaixa.ws;

public class Content {
	
	private String cid;
	private int price;
	private byte[] dados;
		
	
	public Content(String cid, int price, byte[] dados) {
		this.cid = cid;
		this.price = price;
		this.dados = dados;
	}
	
	public byte[] getDados() {
		return dados;
	}
	
	public void setInfo(byte[] content) {
		this.dados = content;
	}
	
	public int getPrice() {
		return price;
	}
	
	public void setPrice(int price) {
		this.price = price;
	}
	
	public String getCid() {
		return cid;
	}
	
	public void setCid(String cid) {
		this.cid = cid;
	}
}