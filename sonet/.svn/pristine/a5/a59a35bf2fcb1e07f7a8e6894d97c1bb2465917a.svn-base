      </div>
    </noscript>


	<table class="layout" cellspacing="0" width="100%" height="100%">
		<tr>
			<td colspan="2" class="header" id="header" height="40%">
				<!--header-->
			</td>
		</tr>
		<tr width="100%" height="60%">
			<!-- left column represents the navigation bar -->
			<td class="column" id="menu" width="30%">
				<!--left column here-->
			</td>
			<!-- right column represents the actual page of Sonet -->
			<td class="content" id="mainPanel" align="center" width="70%">
				<!--content here-->
			</td>
		</tr>
		<tr>
			<footerStyle><td colspan="2" class="footer" id="footer" align="center" >
			</footerStyle>
		</tr>
		
	</table>
	<div>
	
	</div>
</body>
</html>
