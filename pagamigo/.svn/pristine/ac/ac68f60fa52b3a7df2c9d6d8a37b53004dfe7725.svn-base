package pt.pagamigo.ws;

/*Class used to create and manage an client of PagAmigo*/
public class Client {
	private String bankName;
	private String accountID;
	private String name;
	
	
	public Client(String bankName, String accountID, String name) {
		super();
		this.bankName = bankName;
		this.accountID = accountID;
		this.name = name;
	}

	public String getBankName() {
		return this.bankName;
	}

	public void setBankName(String bank) {
		this.bankName = bank;
	}

	public String getAccountID() {
		return this.accountID;
	}
	

	public void setAccountID(String accountID) {
		this.accountID = accountID;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Client [bank=" + bankName + ", conta=" + accountID
				+ ", name=" + name + "]";
	}
	
}
